<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ItemsBrowserFragment">

        <!-- Root folder in content tree. Default value is '/sitecore/content/Home' -->
        <attr name="rootFolder" format="string" />

        <!-- Set to true to show content from cache without setting apiSession -->
        <attr name="loadContentWithoutConnection" format="boolean" />

    </declare-styleable>

    <declare-styleable name="ItemsGridBrowserFragment">

        <!-- Defines column count-->
        <attr name="columnCount" format="integer" />

    </declare-styleable>
</resources>